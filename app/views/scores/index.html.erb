<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Score</th>
      <th>User</th>
    </tr>
  </thead>
  <% if @scores %>
  <tbody>
  <% @scores.each_with_index do |score, index| %>
    <tr>
      <th scope="row"><%= index + 1 %></th>
      <td><%= score.result %></td>
      <td><%= score.user.username %></td>
    </tr>
  <% end %>
  </tbody>
  <% end %>
</table>

